
TARG= libimplode.a
SRCS= implode.c main.c
HDRS= implode.h
OBJS= $(SRCS:.c=.o)

all: $(TARG)

$(TARG): $(OBJS)
	ar cr $(TARG) $(OBJS)

.c.o:
	gcc -O3 -c $*.c

depend: Makefile.in $(SRCS) $(HDRS)
	awk 'BEGIN { copy=1; } { if ( copy ) print } /^#DEPENDENCIES/ { copy=0; }' < Makefile > x
	gcc -M $(INCS) $(SRCS) >> x
	mv x Makefile

clean:
	rm -f $(TARG) $(OBJS) Makefile

#DEPENDENCIES
