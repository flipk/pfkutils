
TARG= libmaketags.a
SRCS= do_logit.c main.c output.c parse.c
HDRS= main.h
OBJS= $(SRCS:.c=.o)

all: $(TARG)

$(TARG): $(OBJS)
	ar cr $(TARG) $(OBJS)

.c.o:
	gcc -Wall -Werror -O3 -c $*.c

depend: Makefile.in $(SRCS) $(HDRS)
	awk 'BEGIN { copy=1; } { if ( copy ) print } /^#DEPENDENCIES/ { copy=0; }' < Makefile > x
	gcc -M $(SRCS) >> x
	mv x Makefile

clean:
	rm -f $(TARG) $(OBJS) Makefile

#DEPENDENCIES
