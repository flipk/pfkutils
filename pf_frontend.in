#!/bin/sh

os=`uname -s`
rev=`uname -r`
pf=$os-$rev

mydir=/home/MY_USERNAME/bin

prog=`basename $0`

if [ ! -d $mydir/$pf ] ; then
	echo platform $pf is not supported by this tool
	exit 1
fi

if [ ! -f $mydir/$pf/$prog ] ; then
	echo a version of program $prog for $os-$rev does not exist
	exit 1
fi

exec $mydir/$pf/$prog $*
