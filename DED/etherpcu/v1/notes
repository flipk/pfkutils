
for f in *.log ; do
  c=1
  rep=1
  min=9999
  max=1
  sum=0
  print -n $f :
  while [[ $rep -eq 1 ]] ; do
    reps=`grep c$c\_m $f | grep 'i am sec' | wc -l`
    c=$(( $c + 1 ))
    if [[ $reps -eq 0 ]] ; then
      rep=0
    else
      [[ $reps -lt $min ]] && min=$reps
      [[ $reps -gt $max ]] && max=$reps
      sum=$(( $sum + $reps ))
      print -n $reps ' '
    fi
  done ; print ''
  avg=$(( $sum / ( $c - 1 ) ))
  print min=$min , max=$max , avg=$avg
done
exit 0

grep WRONG 1 | wc -l

