[push]
	default = simple
[gui]
	fontui = -family \"Source Code Pro\" -size 8 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family \"Source Code Pro\" -size 8 -weight normal -slant roman -underline 0 -overstrike 0
	wmstate = normal
	geometry = 912x772+34+35 329 391
[alias]
	unstage = reset HEAD --
	co = checkout
	cob = checkout -b
	b = branch
	ci = commit
	s = status
	l = log --graph --oneline --decorate --all
	k = !gitk --all &
	sub = submodule
	subfe = submodule foreach
	ssub = !git submodule foreach git status
	la = !git config -l | grep alias | cut -c 7-
	dt = difftool
	dtc = difftool --cached
[diff]
	tool = meld
[merge]
	tool = meld
[difftool]
	prompt = no
